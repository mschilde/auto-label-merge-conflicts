    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.7.4",
    "yargs": "^12.0.5"
  }
